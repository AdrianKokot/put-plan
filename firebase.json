{
  "hosting": {
    "public": "dist/put-plan",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers": [{
        "key": "Access-Control-Allow-Origin",
        "value": "put-plan.web.app"
      }]
    }, {
      "source": "**/*.@(jpg|jpeg|webp|gif|png|css|js|woff|woff2|ttf|ttc|eot|otf)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=31536000"
      }]
    }, {
      "source": "**",
      "headers": [{
        "key": "Content-Security-Policy",
        "value": "default-src 'self'; style-src 'unsafe-inline'; script-src 'unsafe-inline'; frame-src 'self' *.firebasedatabase.app; connect-src 'self' *.firebasedatabase.app; script-src-elem 'self' *.firebasedatabase.app; font-src 'self'; base-uri 'self'; object-src 'none'"
      }]
    }]
  }
}
